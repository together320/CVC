                max-width: 100%;
            }

        .loadercircle {
            border: 5px solid #006bd5;
            border-top: 5px solid #e40000;
            border-radius: 50%;
            width: 100%;
            height: 100%;
            animation: spin 2s linear infinite;
            background-color: #fff;
        }

        @@keyframes spin {
            0% {
                transform: rotate(0deg);
                border-radius: 0%;
            }

            100% {
                transform: rotate(360deg);
                border-radius: 100%;
            }
        }

        #chartdiv g[aria-labelledby='id-66-title'] {
