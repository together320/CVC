                    //{
                    //    socket.server.send('sendheartbeat-' + MachineParameterId_IsHeartBeatSend + "-" + HearBeatSend_Value + "-" + IncrementalNumber_HeartBeatSend + "-" + EndNumber_HeartBeatSend, '');

