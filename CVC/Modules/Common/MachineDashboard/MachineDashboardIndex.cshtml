        $(document).ready(function () {
            var i = 1;
            $("#add_row").click(function () {
                $('#addr' + i).html("<td>" + (i + 1) + "</td><td><input name='name" + i + "' type='text' placeholder='Name' class='form-control input-md'  /> </td><td><input  name='mail" + i + "' type='text' placeholder='Mail'  class='form-control input-md'></td><td><input  name='mobile" + i + "' type='text' placeholder='Mobile'  class='form-control input-md'></td>");

                $('#tab_logic').append('<tr id="addr' + (i + 1) + '"></tr>');
                i++;
            });
            $("#delete_row").click(function () {
                if (i > 1) {
                    $("#addr" + (i - 1)).html('');
                    i--;
                }
            });

            

            
                



        });

        $(function () {

            var counterOnOff = 0;
            var myVar;

            $('#btnStartBatch').click(function () {
                counterOnOff = 1;
                myVar = setInterval(GetSettings, 500);
            });

            $('#btnStopBatch').click(function () {
                counterOnOff = 0;
                clearInterval(myVar);
            });

            $('#btnHeaderOnOff').change(function () {
                //alert('Toggle: ' + $(this).prop('checked'))
                HeaderOnOff($(this).prop('checked'));
            })

            $('#btnSeparatorOnOff').change(function () {
                //alert('Toggle: ' + $(this).prop('checked'))
                SeparatorOnOff($(this).prop('checked'));
            })

            function MachineOnOff() {
                var OnOrOff = "True";
                if ($("#btnStart").css("background-color") == "rgb(255, 0, 0)") {
                    OnOrOff = "False";
                }
                var machine = {"MachineId":2,"OnOrOff":OnOrOff};
                jQuery.support.cors = true;
                $.ajax({
                    url: Q.resolveUrl('~/api/PLCToHMI/MachineOnOff',
                    data: machine,
                    type: 'POST',
                    dataType: 'json',
                    success: function (data) {
                        //WriteResponse(data);
                        //alert(data);
                        if (data == "True") {
                            $("#btnStart").css("background-color", "red");
                            $("#btnStart").html("<i class='fa fa-stop' style='color:white;'></i>Stop Machine");

                        }
                        else {
                            $("#btnStart").css("background-color", "green");
                            $("#btnStart").html("<i class='fa fa-power-off' style='color:white;'></i>Start Machine");
                        }
                    },
                    error: function (x, y, z) {
                        alert(x + '\n' + y + '\n' + z);
                    }
                });
            }

            function HeaderOnOff(OnOrOff) {
                //var OnOrOff = "True";
                //if ($("#btnStart").css("background-color") == "rgb(255, 0, 0)") {
                //    OnOrOff = "False";
                //}
                var machine = { "MachineId": 2, "OnOrOff": OnOrOff };
                jQuery.support.cors = true;
                $.ajax({
