            function GetSettings() {
                var machine = '{MachineId:2}';
                jQuery.support.cors = true;
                $.ajax({
                    url: Q.resolveUrl('~/api/PLCToHMI/Settings',
                    data: machine,
                    type: 'POST',
                    dataType: 'json',
                    success: function (settingsParameter) {
                        //WriteResponse(data);
                        //alert(data);
                        var settingsStringify = JSON.stringify(settingsParameter);
                        var settingsArray = JSON.parse(settingsStringify);
                        var settingsLength = settingsArray.length;

                        //alert(settingsLength);

                        $("#txtProductionSpeed").val(settingsArray[2].ParameterValue);
                        $("#txtStartPosition").val(settingsArray[4].ParameterValue);
                        $("#txtExtension").val(settingsArray[6].ParameterValue);
                        $("#txtDispenserSpeed").val(settingsArray[7].ParameterValue);
                        $("#lblLengthAndGap").val(settingsArray[8].ParameterValue);
                        $("#txtPrinterTemprature").val(settingsArray[12].ParameterValue);
